{
  "name": "kindle",
  "version": "0.0.2",
  "description": "A localstorage key search utility",
  "main": "dist/kindle.js",
  "scripts": {
    "lint": "./node_modules/.bin/tslint -p tslint.json",
    "build": "./node_modules/.bin/tsc",
    "test": "./node_modules/.bin/jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "23.3.9",
    "jest": "23.6.0",
    "ts-jest": "23.10.4",
    "tslint": "5.11.0",
    "typescript": "3.1.6",
    "uuid": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltomes/kindle.git"
  },
  "keywords": [
    "localstorage"
  ],
  "author": "Levi Tomes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ltomes/kindle/issues"
  },
  "homepage": "https://github.com/ltomes/kindle#readme",
  "dependencies": {
    "@types/node": "10.12.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/*.test.+(ts|tsx|js)"
    ]
  }
}
